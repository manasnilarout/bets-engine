{
  "name": "node-starter-boilerplate",
  "version": "1.0.0",
  "description": "A framework to bootstrap development using Node.js API Services with code written in TypeScript.",
  "main": "src/app.ts",
  "scripts": {
    "start": "nps",
    "test": "npm start test",
    "build": "npm start build",
    "presetup": "yarn install",
    "setup": "npm start config && npm start setup.script",
    "lint:ts": "tslint --project tsconfig.json --config tslint.json",
    "lint:fix": "tslint --fix -c tslint.json 'src/**/*{.ts,.tsx}'"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "repository": "https://dev.azure.com/CaleidoHS/Ikanos/_git/node-boilerplate",
  "keywords": [
    "NodeJS",
    "TypeScript",
    "express",
    "boilerplate"
  ],
  "author": {
    "name": "Senthil Arumugam, Raguram Rajagopal, Samuel Sandegren",
    "url": "http://www.inspironics.net/"
  },
  "contributors": [
    {
      "name": "Osmosys Software Solutions",
      "website": " https://osmosys.co/"
    }
  ],
  "husky": {
    "hooks": {
      "pre-commit": "nps lint.fix && nps lint"
    }
  },
  "dependencies": {
    "@types/express": "^4.0.39",
    "caleido-lib": "git+ssh://git@ssh.dev.azure.com:v3/CaleidoHS/Ikanos/caleido-lib",
    "chalk": "^2.4.1",
    "class-validator": "^0.9.1",
    "cls-hooked": "^4.2.2",
    "commander": "^2.19.0",
    "convict": "^5.0.0",
    "copyfiles": "^2.1.0",
    "cors": "^2.8.4",
    "dotenv": "8.0.0",
    "event-dispatch": "0.4.1",
    "exceljs": "^3.4.0",
    "express": "^4.16.2",
    "express-basic-auth": "^1.1.3",
    "express-status-monitor": "^1.0.1",
    "figlet": "^1.2.0",
    "glob": "^7.1.2",
    "helmet": "^3.9.0",
    "jsonfile": "5.0.0",
    "microframework-w3tec": "^0.6.3",
    "morgan": "^1.9.0",
    "multer": "^1.4.2",
    "mysql": "^2.16.0",
    "nanoid": "^2.0.3",
    "ncp": "^2.0.0",
    "nps": "^5.9.3",
    "nps-utils": "^1.5.0",
    "prom-client": "^12.0.0",
    "prometheus-api-metrics": "^2.2.3",
    "pug": "^2.0.4",
    "reflect-metadata": "^0.1.10",
    "routing-controllers": "^0.7.6",
    "shortid": "^2.2.15",
    "swagger-ui-express": "^4.1.2",
    "ts-node": "^8.3.0",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.18",
    "typeorm-typedi-extensions": "^0.2.1",
    "uuid": "^3.3.2",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.10.0",
    "wkhtmltopdf": "^0.3.4"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/test/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "./test/unit/lib/setup.ts"
  },
  "private": true,
  "license": "Contact Osmosys",
  "devDependencies": {
    "@types/amqplib": "^0.5.13",
    "@types/bluebird": "^3.5.18",
    "@types/cls-hooked": "^4.3.0",
    "@types/cors": "^2.8.1",
    "@types/dotenv": "^4.0.2",
    "@types/faker": "^4.1.2",
    "@types/figlet": "^1.2.0",
    "@types/helmet": "^0.0.43",
    "@types/jest": "^23.3.2",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.10",
    "@types/nock": "^9.1.3",
    "@types/pug": "^2.0.4",
    "@types/redis": "^2.8.13",
    "@types/supertest": "^2.0.4",
    "@types/uuid": "^3.4.3",
    "cross-env": "^5.1.1",
    "faker": "^4.1.0",
    "husky": "^3.0.2",
    "jest": "^23.6.0",
    "mock-express-request": "^0.2.0",
    "mock-express-response": "^0.2.1",
    "multi-file-swagger": "^2.3.0",
    "nock": "^10.0.0",
    "nodemon": "^1.12.1",
    "sqlite3": "^4.0.0",
    "supertest": "^3.0.0",
    "ts-jest": "^23.10.1",
    "tslint": "^5.19.0",
    "typeorm-seeding": "^1.0.0-rc.3",
    "typeorm-typedi-extensions": "^0.2.1",
    "typescript": "3.5.2"
  }
}

{
    // Use IntelliSense to learn about possible Node.js debug attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Node: Nodemon",
            "address": "0.0.0.0",
            "port": 9119,
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "/vagrant/code/node-starter-boilerplate",
            "sourceMaps": true,
            "restart": true
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Node: Pegasus Remote Debugging",
            "processId": "7086",
            "protocol": "inspector"
        }
    ]
}
